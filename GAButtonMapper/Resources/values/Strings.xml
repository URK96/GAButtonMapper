  <string name="AlertDialog_DoNotDisturb_Title">Caution</string>
  <string name="AlertDialog_DoNotDisturb_Message">
    This feature requires extra permission because of Do Not Disturb feature.\n
    If you select Change Permission, you can go to the Settings screen and activate GAButtonMapper.\n
    The existing silent mode is changed to Do Not Disturb mode, and the detailed setting can be set in Do Not Disturb mode setting.
  </string>
  <string name="AlertDialog_DoNotDisturb_OK">Change Permission</string>
  <string name="AlertDialog_IgnoreBatteryOptimization_Title">Caution</string>
  <string name="AlertDialog_IgnoreBatteryOptimization_Message">
    This setting is intended to use remap feature more reliably.\n
    Enabling this setting will significantly reduce the frequency at which the service automatically pauses, but may result in a slight increase in battery consumption.\n
    If you select Change Setting, go to the setup screen and enable/disable GAButtonMapper for activation/disabling.\n\n
  </string>
  <string name="AlertDialog_IgnoreBatteryOptimization_OK">Change Setting</string>
